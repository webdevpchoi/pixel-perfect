<%- include('../partials/header') %>
<h2 class="text-center m-3">All Photographers</h2>
<a href="/photographers/new">
	<button class="btn btn-primary">Add Photographer</button>
</a>

<div class="row">
	<% photographers.forEach((photographer) => { %>
		<div class="photographer col-md-3 col-md-6">
			<a href="/photographers/<%= photographer._id %>">
				<img src="<%= photographer.profileImg %>" alt="" class="img-thumbnail">
			</a>
		</div>
	<% }) %>
</div>
<%- include('../partials/footer') %>
